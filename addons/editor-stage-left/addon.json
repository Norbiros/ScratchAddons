{
  "name": "Display stage on left side",
  "description": "Moves the stage to the left side of the editor.",
  "credits": [
    {
      "name": "NitroCipher/ZenithRogue"
    },
    {
      "name": "djdolphin"
    }
  ],
  "userscripts": [
    {
      "url": "fix-share-the-love.js",
      "matches": ["projects"]
    }
  ],
  "dynamicEnable": true,
  "dynamicDisable": true,
  "injectAsStyleElt": true,
  "updateUserstylesOnSettingsChange": true,
  "userstyles": [
    {
      "url": "stageleft.css",
      "matches": ["projects"]
    },
    {
      "url": "fullscreen-on-left.css",
      "matches": ["projects"],
      "if": {
        "settings": { "fullscreenOnLeft": true }
      }
    }
  ],
  "settings": [
    {
      "name": "Fullscreen button on left",
      "id": "fullscreenOnLeft",
      "type": "boolean",
      "default": false
    }
  ],
  "tags": ["editor", "theme", "featured"],
  "versionAdded": "1.1.0",
  "enabledByDefault": false
}
